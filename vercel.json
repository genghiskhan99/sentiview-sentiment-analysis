{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_API_URL": "@sentiview-api-url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@sentiview-api-url"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "$NEXT_PUBLIC_API_URL/:path*"
    }
  ]
}
